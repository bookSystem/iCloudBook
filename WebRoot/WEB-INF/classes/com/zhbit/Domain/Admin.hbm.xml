<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.zhbit.Domain.Admin" table="t_admin">
		<id name="adminId">
			<generator class="native"></generator>
		</id>
		
		<property name="name" column="adminName" not-null="true"></property>
		<property name="password" column="adminPassword" not-null="true"></property>
	</class>
</hibernate-mapping>


